{
  "name": "data-subscription",
  "version": "1.0.0",
  "description": "DS API Data Subscription Service",
  "scripts": {
    "start": "node bin/start.js",
    "dev": "tsnd src/start.ts",
    "clean": "./scripts/clean.sh",
    "build": "npm run tsbuild",
    "tsbuild": "tsc --project tsconfig.build.json",
    "tswatch": "tsc --watch --preserveWatchOutput",
    "eslint": "eslint .",
    "eslint-fix": "eslint --fix .",
    "lint": "npm run eslint",
    "audit": "npm audit --parseable --audit-level=high || true",
    "jest": "NODE_OPTIONS=--max-old-space-size=8192 jest --config jest.json --logHeapUsage --detectOpenHandles",
    "test": "npm run eslint && TZ=GMT npm run jest && npm run audit"
  },
  "license": "ISC",
  "dependencies": {
    "@sap/audit-logging": "^6.1.0",
    "@sap/xsenv": "^5.2.0",
    "@bdc-fos/fos-logger": "^1.0.10",
    "ajv": "^8.16.0",
    "ajv-formats": "^3.0.1",
    "axios": "^1.7.2",
    "express": "^5.0.1",
    "helmet": "^8.0.0",
    "http-status-codes": "^2.3.0",
    "node-forge": "^1.3.1",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "pg": "^8.12.0",
    "sequelize": "^6.37.3",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@bdc-fos/eslint-config": "^9.0.4",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/node-forge": "^1.3.11",
    "@types/passport": "^1.0.16",
    "@types/passport-strategy": "^0.2.38",
    "@types/sap__xsenv": "^3.3.2",
    "@types/supertest": "^6.0.2",    
    "eslint": "^9.18.0",   
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "semver": "7.7.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  }
